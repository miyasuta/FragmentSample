<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="{i18n>Suppliers}">
		<content>
			<List items="{/Suppliers}">
				<items>
					<StandardListItem
						title="{SupplierID}"
						description="{CompanyName}"
						type="Active" 
						press="onItemPress" />
				</items>
			</List>
		</content>
	</Dialog>
</core:FragmentDefinition>